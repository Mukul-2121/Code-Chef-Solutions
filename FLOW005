#include <iostream>
using namespace std;

int main(){
   int t, n, cnt = 0;
   cin >> t;
   for (int i = 0 ; i < t ; i++){
      cin >> n;
      while(n > 0){
         if (n >= 100){
            n -= 100;
            cnt++;
         }
         else if (n >= 50){
            n -= 50;
            cnt++;
         }
         else if (n >= 10){
            n -= 10;
            cnt++;
         }
         else if (n >= 5){
            n -= 5;
            cnt++;
         }
         else if (n >= 2){
            n -= 2;
            cnt++;
         }
         else{
            n -= 1;
            cnt++;
         }
      }
      cout << cnt << endl;
      cnt = 0;
   }
}
