Problem name : Farmer Feb 
#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t,x,y,sum,p,flag;
	cin>>t;
	while(t--)
	{
	    cin>>x>>y;
	    sum=x+y;
	    p=sum+1;
	    while(1)
	    {
	        flag=0;
	        for(int i=2;i*i<=p;i++)
	        {
	            if(p%i==0)
	            {
	                flag=1;
	                break;
	            }
	        }
	        if(flag==0)
	        {
	            cout<<p-sum<<endl;
	            break;
	        }
	        else
	        p++;
	    }
	    
	}
	
	return 0;
}
